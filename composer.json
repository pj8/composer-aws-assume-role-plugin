{
    "name": "pj8/aws-assume-role-plugin",
    "license": "MIT",
    "type": "composer-plugin",
    "keywords": [
        "composer",
        "tool",
        "aws",
        "role",
        "executable"
    ],
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.0",
        "aws/aws-sdk-php": "^3.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Pj8\\AwsAssumeRolePlugin\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "class": "Pj8\\AwsAssumeRolePlugin\\Plugin"
    }
}
